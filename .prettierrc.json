{
	"printWidth": 120,
	"semi": true,
	"useTabs": true,
	"vueIndentScriptAndStyle": false,
	"singleQuote": false,
	"trailingComma": "all",
	"proseWrap": "never",
	"htmlWhitespaceSensitivity": "ignore",
	"endOfLine": "lf",
	"arrowParens": "avoid",
	"quoteProps": "consistent",
	"bracketSpacing": true
}
